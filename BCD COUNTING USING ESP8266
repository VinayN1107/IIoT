void setup() { 
  // put your setup code here, to run once: 
pinMode(D7,OUTPUT); 
pinMode(D6,OUTPUT); 
pinMode(D5,OUTPUT);  } 
void loop()   { 
digitalWrite(D5, LOW); 
digitalWrite(D6,LOW); 
digitalWrite(D7,LOW); 
delay(500); 
digitalWrite(D5, LOW); 
digitalWrite(D6,LOW); 
digitalWrite(D7,HIGH); 
delay(500); 
digitalWrite(D5, LOW); 
digitalWrite(D6,HIGH); 
digitalWrite(D7,LOW); 
delay(500); 
digitalWrite(D5, LOW); 
digitalWrite(D6,HIGH); 
digitalWrite(D7,HIGH); 
delay(500); 
digitalWrite(D5, HIGH); 
digitalWrite(D6,LOW); 
digitalWrite(D7,LOW); 
delay(500); 
digitalWrite(D5, HIGH); 
digitalWrite(D6,LOW); 
digitalWrite(D7,HIGH); 
delay(500); 
digitalWrite(D5, HIGH); 
digitalWrite(D6,HIGH); 
digitalWrite(D7,LOW); 
delay(500); 
digitalWrite(D5, HIGH); 
digitalWrite(D6,HIGH); 
digitalWrite(D7,HIGH); 
delay(500);  } 
